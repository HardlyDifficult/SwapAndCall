{
  "name": "swap-and-call",
  "version": "0.0.1",
  "description": "Uniswap ETH or ERC-20 tokens into the ERC-20 token a 3rd party contract expects, then refund any remainder.",
  "scripts": {
    "build": "npm run compile",
    "compile": "truffle compile",
    "test": "truffle test",
    "lint": "solium -d contracts --fix && eslint . --fix",
    "lintNoFix": "solium -d contracts && eslint .",
    "coverage": "truffle run coverage && cat coverage/lcov.info | coveralls",
    "ganache": "ganache-cli -k istanbul"
  },
  "files": [
    "build",
    "contracts",
    "src",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HardlyDifficult/swap-and-call.git"
  },
  "keywords": [
    "Solidity",
    "Uniswap",
    "Ethereum",
    "ETH"
  ],
  "author": "HardlyDifficult",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HardlyDifficult/swap-and-call/issues"
  },
  "homepage": "https://github.com/HardlyDifficult/swap-and-call/#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "@vue/cli": "4.1.1",
    "bignumber.js": "9.0.0",
    "hardlydifficult-ethereum-contracts": "0.7.7"
  },
  "devDependencies": {
    "@codechecks/client": "0.1.10",
    "babel-eslint": "10.0.3",
    "chai": "4.2.0",
    "coveralls": "3.0.9",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-markdown": "1.0.1",
    "eslint-plugin-prettier": "3.1.1",
    "eth-gas-reporter": "0.2.14",
    "ganache-cli": "6.8.0-istanbul.0",
    "prettier": "1.19.1",
    "solidity-coverage": "0.7.0-beta.3",
    "solium": "1.2.5",
    "truffle": "5.1.4",
    "truffle-assertions": "0.9.2"
  }
}
